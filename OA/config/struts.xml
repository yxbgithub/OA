<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<constant name="devMode" value="true"/>
	<constant name="struts.action.extension" value="do"/>
    <package name="user" namespace="/registration" extends="struts-default">
	    <action name="userAction" class="userAction" method="add">
	    	<result name="failed" type="dispatcher">/registerFailed.jsp</result>
	    	<result name="success" type="dispatcher">/registerSuccess.jsp</result>
	    	<result name="load" type="dispatcher">/user.jsp</result>
	    </action>
	    	    
    </package>
</struts>
